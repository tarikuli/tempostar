<?xml version="1.0"?>
<!-- Declares the Hanesce_TempostarConnector module and its dependency on Magento_Sales. -->
<!-- Required for Magento to recognize and register the module. -->
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Module/etc/module.xsd">
    <module name="Hanesce_TempostarConnector" setup_version="1.0.1">
        <sequence>
            <module name="Magento_Sales"/>
        </sequence>
    </module>
</config>
